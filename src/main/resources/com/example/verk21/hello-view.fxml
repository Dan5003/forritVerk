<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="CENTER" prefHeight="462.0" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.verk21.HelloController">
   <padding>
      <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
   </padding>
   <children>
      <HBox prefHeight="114.0" prefWidth="550.0" spacing="5.0">
         <children>
            <TextArea fx:id="fxLeitarord" onKeyTyped="#onLeitarord" prefHeight="300.0" prefWidth="170.0" promptText="Write Search word here..." style="-fx-background-color: #ffffff; -fx-border-color: #6b6a6a; -fx-border-width: 2;" HBox.hgrow="ALWAYS" />
            <Label fx:id="fxStadsetningOrds" alignment="CENTER" contentDisplay="CENTER" prefHeight="300.0" prefWidth="170.0" style="-fx-background-color: #ffffff; -fx-border-color: #6b6a6a; -fx-border-width: 2; -fx-padding: 4;" text="0" textAlignment="CENTER" />
            <Button mnemonicParsing="false" onAction="#onLeit" prefHeight="300.0" prefWidth="150.0" text="Search" />
         </children>
      </HBox>
      <HBox prefHeight="171.0" prefWidth="550.0" spacing="5.0">
         <children>
            <TextArea fx:id="fxTextinn" onKeyTyped="#onTextaBox" prefHeight="300.0" prefWidth="395.0" promptText="write your text here..." style="-fx-background-color: #ffffff; -fx-border-color: #6b6a6a; -fx-border-width: 2;" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#onVistaTexta" prefHeight="300.0" prefWidth="150.0" text="Save" />
         </children>
      </HBox>
      <HBox prefHeight="115.0" prefWidth="550.0" spacing="5.0">
         <children>
            <Label fx:id="fxFjoldiOrda" alignment="CENTER" prefHeight="300.0" prefWidth="395.0" style="-fx-background-color: #ffffff; -fx-border-color: #6b6a6a; -fx-border-width: 2; -fx-padding: 4;" text="0" />
            <Button mnemonicParsing="false" onAction="#onTeljaOrd" prefHeight="100.0" prefWidth="150.0" text="Count" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="136.0" prefWidth="550.0" spacing="5.0">
         <children>

            <TextArea fx:id="fxMeaning" prefHeight="100.0" prefWidth="240.0" promptText="Type your query here..." style="-fx-background-color: #ffffff; -fx-border-color: #6b6a6a; -fx-border-width: 2;" />
            <Button fx:id="fxDictionaryCheck" onAction="#onDictionaryCheck" prefHeight="100.0" prefWidth="150.0" text="Dictionary" />
            <Button layoutX="210.0" layoutY="10.0" onAction="#onGetRandomWord" prefHeight="100.0" prefWidth="150.0" text="Random Word" />
         </children>
      </HBox>
      <HBox prefHeight="161.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="122.0" prefWidth="625.0" style="-fx-background-color: #ffffff; -fx-border-color:#6b6a6a; -fx-border-width: 2;">
               <children>
                  <Label fx:id="dictName" wrapText="true">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
